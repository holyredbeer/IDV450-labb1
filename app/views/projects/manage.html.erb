<h1>Hantera projekt</h1>

<div id="project_area">
	<% @users_projects.each do |project| %>
		<h4>
			<%= link_to project.title, project_path(project), :class => "manage_project_link" %>
			<%= link_to "Radera", project, :method=>:delete, :class => "btn remove_button", :confirm => "Are you sure?" %>
			<%= link_to "Redigera", edit_project_path(project), :class => "btn edit_button" %>
			<br/>
		</h4>
	<% end %>
</div>